program MultiForm;

uses
  Vcl.Forms,
  ufMain in 'ufMain.pas' {fMain},
  ufTicket in 'ufTicket.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfMain, fMain);
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
